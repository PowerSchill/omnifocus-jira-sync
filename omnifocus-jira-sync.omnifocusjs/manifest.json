{
    "identifier": "com.omnifocus.plugin.jira-sync",
    "author": "OmniFocus Jira Sync",
    "version": "1.2.0",
    "description": "One-way sync from Jira to OmniFocus using JQL queries",
    "defaultLocale": "en",
    "libraries": [
        {
            "identifier": "jiraCommon"
        }
    ],
    "actions": [
        {
            "identifier": "configureJira",
            "image": "gearshape.fill"
        },
        {
            "identifier": "syncJira",
            "image": "arrow.triangle.2.circlepath"
        },
        {
            "identifier": "syncJiraFull",
            "image": "arrow.triangle.2.circlepath.circle.fill"
        }
    ]
}